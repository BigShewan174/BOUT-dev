# Test of the FieldFactory class 
#
# Generates a set of fields and writes them to output file
#

NOUT = 0  # No timesteps

MZ = 32    # Z size

#grid = "test_laplace.grd.nc"

dump_format = "nc"  # NetCDF format. Alternative is "pdb"

[mesh]
#nx = 388
nx = 12
#dx = 0.01
ny = 128
symmetricGlobalX = false
symmetricGlobalY = false

[laplace]
all_terms = false
include_yguards = false
filter = 0.2
type = ptri
maxits = 100000
rtol = 1e-12
#B = 0.0207 # 4 procs, 42 its
#B = 0.4
#om = 1.1433
# kz = 0 mode
#om = 1.19
#B = 0.55
omega = 0.5
B = 20
new_method=false
kz_direct_solve = 256

[output]
floats = true
